<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/Base.TextAppearance.AppCompat.Widget.ActionBar.Title"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ertty"
    tools:context="com.example.sahin.navigasyon.BiletAl">


    <ImageButton
        android:id="@+id/btnimgUckbltal"
        android:layout_width="66dp"
        android:layout_height="66dp"

        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="43dp"
        android:layout_marginTop="143dp"
        android:contentDescription="Uçak Bileti Al"
        app:srcCompat="@drawable/rsz_ucakrezervasyonu" />

    <TextView
        android:id="@+id/textView3"
        style="@style/Widget.AppCompat.TextView.SpinnerItem"
        android:layout_width="wrap_content"

        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView6"
        android:layout_below="@+id/btnimgUckbltal"
        android:layout_marginTop="11dp"
        android:text="Uçak Bileti Al"
        android:textStyle="bold|italic" />

    <ImageButton
        android:id="@+id/btnimgotbsbal"
        android:layout_width="70dp"
        android:layout_height="66dp"
        android:layout_alignTop="@+id/btnimgUckbltal"
        android:layout_centerHorizontal="true"
        app:srcCompat="@drawable/rsz_otobusbilet" />

    <TextView
        android:id="@+id/textView"
        style="@style/Widget.AppCompat.TextView.SpinnerItem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView3"
        android:layout_alignBottom="@+id/textView3"
        android:layout_alignEnd="@+id/btnimgotbsbal"
        android:layout_alignStart="@+id/btnimgotbsbal"
        android:text="Otobüs Bileti Al"
        android:textStyle="bold|italic" />

    <ImageButton
        android:id="@+id/btnimgarckrala"
        android:layout_width="66dp"
        android:layout_height="66dp"
        android:layout_alignBottom="@+id/btnimgotbsbal"
        android:layout_marginStart="42dp"
        android:layout_toEndOf="@+id/btnimgotbsbal"
        app:srcCompat="@drawable/rsz_arackiralama" />

    <TextView
        android:id="@+id/textView2"
        style="@style/Widget.AppCompat.TextView.SpinnerItem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView"
        android:layout_alignBottom="@+id/textView"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="13dp"
        android:text="Araç Kiralama"
        android:textStyle="bold|italic" />

    <ImageButton
        android:id="@+id/btnimgtrnblal"
        android:layout_width="66dp"
        android:layout_height="66dp"
        android:layout_alignStart="@+id/btnimgUckbltal"
        android:layout_below="@+id/textView"
        android:layout_marginTop="29dp"
        app:srcCompat="@drawable/rsz_train" />

    <TextView
        android:id="@+id/textView6"
        style="@style/Widget.AppCompat.TextView.SpinnerItem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/btnimgtrnblal"
        android:layout_marginStart="19dp"
        android:layout_marginTop="11dp"
        android:text="Tren Seferleri"
        android:textStyle="bold|italic" />

    <ImageButton
        android:id="@+id/imgbtnhotelrzvs"
        android:layout_width="66dp"
        android:layout_height="66dp"
        android:layout_above="@+id/textView6"
        android:layout_alignStart="@+id/textView"
        app:srcCompat="@drawable/rsz_hotelim" />

    <TextView
        android:id="@+id/textView9"
        style="@style/Widget.AppCompat.TextView.SpinnerItem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView6"
        android:layout_alignBottom="@+id/textView6"
        android:layout_centerHorizontal="true"
        android:text="Otel Bul"
        android:textStyle="bold|italic"
        android:visibility="visible" />

    <ImageButton
        android:id="@+id/btnimgturlar"
        android:layout_width="66dp"
        android:layout_height="66dp"
        android:layout_alignStart="@+id/btnimgarckrala"
        android:layout_alignTop="@+id/imgbtnhotelrzvs"
        app:srcCompat="@drawable/rsz_turlar" />

    <TextView
        android:id="@+id/textView10"
        style="@style/Widget.AppCompat.TextView.SpinnerItem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView6"
        android:layout_alignBottom="@+id/textView6"
        android:layout_alignStart="@+id/btnimgturlar"
        android:text="Turlar"
        android:textStyle="bold|italic" />


</RelativeLayout>
